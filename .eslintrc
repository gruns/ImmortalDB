{
    "parserOptions": {
      "ecmaVersion": "2018",
      "sourceType": "module"
    },
    "plugins":["prettier"],
    "extends": [
      "standard", "eslint-config-prettier"
    ],
    "rules": {
        "space-before-function-paren": [2, "always"]
    },
    "env": {
        "browser": true,
        "es6": true
    }
}
